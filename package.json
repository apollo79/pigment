{
  "name": "@kobalte/pigment",
  "version": "0.0.1",
  "private": true,
  "description": "Beautifully designed components built with Kobalte and Tailwind CSS.",
  "keywords": [
    "solid",
    "solidjs",
    "ui",
    "library",
    "design-system",
    "components",
    "tailwind"
  ],
  "homepage": "https://github.com/kobaltedev/pigment/tree/main#readme",
  "bugs": {
    "url": "https://github.com/kobaltedev/pigment/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kobaltedev/pigment.git"
  },
  "license": "MIT",
  "author": "Fabien Marie-Louise <fabienml.dev@gmail.com>",
  "type": "module",
  "scripts": {
    "build": "solid-start build",
    "clean": "rm -rf .solid && rm -rf netlify && rm -rf node_modules && rm -rf dist",
    "commit": "git-cz",
    "dev": "solid-start dev",
    "format": "prettier . --write --ignore-path .gitignore && git update-index --again",
    "prepare": "husky install",
    "start": "solid-start start"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --write"
    ],
    "package.json": "npx sort-package-json"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@kobalte/core": "0.6.1",
    "@kobalte/utils": "0.5.1",
    "@solidjs/meta": "0.28.2",
    "@solidjs/router": "0.7.0",
    "class-variance-authority": "0.4.0",
    "clsx": "1.2.1",
    "lucide-solid": "0.112.0",
    "solid-js": "1.6.10",
    "solid-start": "0.2.20",
    "undici": "5.18.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@commitlint/cz-commitlint": "17.4.2",
    "@kobalte/tailwindcss": "0.4.2",
    "@mdx-js/mdx": "2.2.1",
    "@mdx-js/rollup": "2.2.1",
    "@types/node": "18.13.0",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "acorn": "8.8.2",
    "autoprefixer": "10.4.13",
    "commitizen": "4.3.0",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-solid": "0.9.4",
    "github-slugger": "2.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.1",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.2",
    "rehype-pretty-code": "0.9.3",
    "rehype-raw": "6.1.1",
    "rehype-slug": "5.1.0",
    "remark-gfm": "3.0.1",
    "remark-shiki-twoslash": "3.1.0",
    "shiki": "0.14.0",
    "solid-mdx": "0.0.6",
    "solid-start-netlify": "0.2.20",
    "solid-start-node": "0.2.20",
    "sort-package-json": "2.4.1",
    "tailwindcss": "3.2.6",
    "tailwindcss-animate": "1.0.5",
    "typescript": "4.9.5",
    "unist-util-visit": "4.1.2",
    "vite": "3.2.5"
  },
  "packageManager": "pnpm@7.26.3",
  "engines": {
    "node": ">=16"
  }
}
